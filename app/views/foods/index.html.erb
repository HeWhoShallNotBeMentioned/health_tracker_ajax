<p id="notice"><%= notice %></p>

<h1>My Foods</h1>
<%= form_tag foods_path, :method => 'get', :id => "foods_search" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>

  <div id="foods"><%= render 'foods' %></div>
<% end %>


<%= link_to 'New Food', new_food_path %>

<br>
<%= will_paginate @foods %>

<br>
<div id="exercises"><%= render 'exercises/exercises' %></div>
<%= link_to 'New Exercise', new_exercise_path %>

<br>
<%= will_paginate @exercises %>
<br>
